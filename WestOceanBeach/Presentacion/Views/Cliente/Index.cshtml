@model Entities.Entities.Cliente
<div>
    <a class="ui-btn ui-shadow ui-corner-all ui-btn-inline ui-mini" style="float:right">Prueba</a>
</div>

<div class="d-flex justify-content-center">

    <form asp-controller="Cliente" asp-action="insertarCliente">
        <div class="form-group row">
           
            <div class="col-sm-10">
                <label asp-for="cedula" class="control-label">Cedula:</label>
                <input asp-for="cedula" type="text" class="form-control" required>

            </div>
        </div>
        <div class="form-group row">
         
            <div class="col-sm-10">
                <label asp-for="nombre" class="control-label">Nombre:</label>
                <input asp-for="nombre" type="text" class="form-control" required>
            </div>
        </div>
        <div class="form-group row">
           
            <div class="col-sm-10">
                <label asp-for="correo" class="control-label">Correo:</label>
                <input asp-for="correo" type="text" class="form-control" required>
            </div>
        </div>

        <div class="text-center">
            <input type="submit" value="Insertar" class="btn btn-primary" style="background-color: #005cb9;" />

        </div>

    </form>
    <div> prueba Valeska</div>
</div>
